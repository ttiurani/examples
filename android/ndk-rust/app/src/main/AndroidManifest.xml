    android:versionCode="1"
    android:versionName="1.0" >

  <uses-sdk
      android:minSdkVersion="21"
      android:targetSdkVersion="32" />

  <application
      android:label="Example NDK Rust App"
      android:theme="@style/Theme.Example">
